package com.basicprogram;

public class JavaOne {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
for (int i=2;i<100;i++) {
	boolean a = true;
	for(int j=2;j<i;j++) {
		if (i%j==0) {
		a = false;
		break;
	}
		if(a==true);
		System.out.println(i);
		break;
	}

}
	}

}
